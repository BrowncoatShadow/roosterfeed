- var query = token ? '?token=' + token : ''
title Rooster Feed
each feed in feeds
  link(
    rel='alternate'
    title=feed.title
    type='application/json'
    href='/' + feed.name + '.json' + query
  )
h1 Rooster Teeth JSON Feeds
ul
  each feed in feeds
    li
      a(href='/' + feed.name + '.json' + query) #{feed.title}
